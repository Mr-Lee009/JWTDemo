		{
			"name": "signin Token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Get access token return status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Get access token has value\", function () {\r",
							"    const jsonData = pm.response.json();\r",
							"\r",
							"    pm.expect(jsonData).to.have.property('accessToken');\r",
							"    pm.environment.set(\"token\", jsonData.id);\r",
							"    \r",
							"    pm.expect(jsonData).to.have.property('refreshToken');\r",
							"    pm.environment.set(\"refresh_token\", jsonData.refreshToken);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
